ent={SignUp} />\n          {/* <Route exact path=\"/\" component={Home} />\n          <Route exact path=\"/dashboard\" component={Dashboard} />\n          <Route exact path=\"/login\" component={LogIn} />\n          <Route exact path=\"/signup\" component={SignUp} /> */}\n        </Switch>\n      </Router>\n    </AuthProvider>\n  );\n};\n\nexport default App;\n","/home/dci/learningToProgram/Web-Development/DCI/SPA/1st-Project/Food-Rating-App/food-rating-app/src/data/data.js",[],"/home/dci/learningToProgram/Web-Development/DCI/SPA/1st-Project/Food-Rating-App/food-rating-app/src/components/LogIn.jsx",["93"],"import React, { useState, useEffect } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { users } from \"../data/data\";\nimport ErrorMessage from \"./ErrorMessage\";\nimport { AiFillEye, AiFillEyeInvisible } from \"react-icons/ai\";\nimport {\n  Input,\n  Button,\n  FormControl,\n  FormLabel,\n  InputGroup,\n  InputRightElement,\n} from \"@chakra-ui/react\";\n\nconst LogIn = () => {\n  const [userDetails, setUserDetails] = useState({\n    userName: \"\",\n    password: \"\",\n  });\n  const [isUser, setIsUser] = useState(false);\n  const [user, setUser] = useState(users);\n  const [show, setShow] = React.useState(false);\n  const [error, setError] = useState(\"\");\n  const handleClick = () => setShow(!show);\n\n  const signInHandler = (e) => {\n    setUserDetails({ ...userDetails, [e.target.name]: e.target.value });\n  };\n\n  const submitHandler = (e) => {\n    e.preventDefault();\n    const userNameCheck = users.filter(\n      (user) => user.userName === userDetails.userName\n    );\n    console.log(userDetails.password);\n    if (\n      userNameCheck[0] &&\n      userNameCheck[0].password === userDetails.password\n    ) {\n      setIsUser(true);\n      setError(\"\");\n    } else {\n      setError(\"Invalid username or password\");\n    }\n  };\n\n  useEffect(() => {\n    if (isUser !== true) {\n      setIsUser(false);\n    } else {\n      setIsUser(true);\n    }\n  }, [isUser]);\n\n  return (\n    <form autoComplete=\"off\" className=\"logInContainer\">\n      <div className=\"submitContainer\">\n        {error && <ErrorMessage message={error} />}\n        <FormControl isRequired id=\"userName\">\n          <FormLabel colorScheme=\"teal\" className=\"form-label\">\n            Username\n          </FormLabel>\n          <Input\n            color=\"white\"\n            type=\"text\"\n            name=\"userName\"\n            placeholder=\"Your user name\"\n            onChange={signInHandler}\n            required\n          />\n        </FormControl>\n        <FormControl isRequired id=\"userName\" py={4}>\n          <FormLabel className=\"form-label\">Password</FormLabel>\n          <InputGroup size=\"md\">\n            <Input\n              color=\"white\"\n              pr=\"4.5rem\"\n              name=\"password\"\n              type={show ? \"text\" : \"password\"}\n              placeholder=\"Enter password\"\n              onChange={signInHandler}\n            />\n            <InputRightElement width=\"4.5rem\">\n              <Button h=\"1.75rem\" size=\"sm\" onClick={handleClick}>\n                {show ? <AiFillEyeInvisible /> : <AiFillEye />}\n              </Button>\n            </InputRightElement>\n          </InputGroup>\n        </FormControl>\n        <Button mt={4} onClick={submitHandler}>\n          <Link\n            className=\"link\"\n            to={(isUser && `/User/${user[0].userName}`) || \"/\"}\n          >\n            Log In\n          </Link>\n        </Button>\n      </div>\n    </form>\n  );\n};\n\nexport default LogIn;\n","/home/dci/learningToProgram/Web-Development/DCI/SPA/1st-Project/Food-Rating-App/food-rating-app/src/components/User.jsx",[],"/home/dci/learningToProgram/Web-Development/DCI/SPA/1st-Project/Food-Rating-App/food-rating-app/src/components/Home.jsx",[],"/home/dci/learningToProgram/Web-Development/DCI/SPA/1st-Project/Food-Rating-App/food-rating-app/src