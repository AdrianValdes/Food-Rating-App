e} />\n          <Route exact path=\"/user/:user/\" component={User} />\n          <Route exact path=\"/signup\" component={SignUp} />\n          <Route exact path=\"/login\" component={LogIn} />\n          {/*<Route exact path=\"/dashboard\" component={Dashboard} />\n          <Route exact path=\"/login\" component={LogIn} />\n          <Route exact path=\"/signup\" component={SignUp} /> */}\n        </Switch>\n      </Router>\n    </AuthProvider>\n  );\n};\n\nexport default App;\n","/home/dci/learningToProgram/Web-Development/DCI/SPA/1st-Project/Food-Rating-App/food-rating-app/src/data/data.js",[],"/home/dci/learningToProgram/Web-Development/DCI/SPA/1st-Project/Food-Rating-App/food-rating-app/src/components/User.jsx",[],"/home/dci/learningToProgram/Web-Development/DCI/SPA/1st-Project/Food-Rating-App/food-rating-app/src/components/authentication/Auth.jsx",[],"/home/dci/learningToProgram/Web-Development/DCI/SPA/1st-Project/Food-Rating-App/food-rating-app/src/components/authentication/Dashboard.jsx",[],"/home/dci/learningToProgram/Web-Development/DCI/SPA/1st-Project/Food-Rating-App/food-rating-app/src/components/LogIn.jsx",["94"],"import React, { useState, useEffect } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { users } from \"../data/data\";\nimport ErrorMessage from \"./ErrorMessage\";\nimport { AiFillEye, AiFillEyeInvisible } from \"react-icons/ai\";\nimport {\n  Input,\n  Button,\n  FormControl,\n  FormLabel,\n  InputGroup,\n  InputRightElement,\n  Heading,\n  Flex,\n  Spacer,\n  Text,\n} from \"@chakra-ui/react\";\n\nconst LogIn = () => {\n  const [userDetails, setUserDetails] = useState({\n    userName: \"\",\n    password: \"\",\n  });\n  const [isUser, setIsUser] = useState(false);\n  const [user, setUser] = useState(users);\n  const [show, setShow] = React.useState(false);\n  const [error, setError] = useState(\"\");\n  const handleClick = () => setShow(!show);\n\n  const signInHandler = (e) => {\n    setUserDetails({ ...userDetails, [e.target.name]: e.target.value });\n  };\n\n  const submitHandler = (e) => {\n    e.preventDefault();\n    const userNameCheck = users.filter(\n      (user) => user.userName === userDetails.userName\n    );\n    console.log(userDetails.password);\n    if (\n      userNameCheck[0] &&\n      userNameCheck[0].password === userDetails.password\n    ) {\n      setIsUser(true);\n      setError(\"\");\n    } else {\n      setError(\"Invalid username or password\");\n    }\n  };\n\n  useEffect(() => {\n    if (isUser !== true) {\n      setIsUser(false);\n    } else {\n      setIsUser(true);\n    }\n  }, [isUser]);\n\n  return (\n    <form className=\"logInContainer\">\n      <div className=\"submitContainer\">\n        {error && <ErrorMessage message={error} />}\n        <Heading\n          as=\"h1\"\n          my=\"5\"\n          color=\"teal.400\"\n          fontSize=\"4xl\"\n          fontWeight=\"extrabold\"\n          textAlign=\"center\"\n        >\n          Enter Your LogIn Details\n        </Heading>\n        <FormControl isRequired id=\"userName\">\n          <FormLabel className=\"form-label\">Username</FormLabel>\n          <Input\n            color=\"white\"\n            type=\"text\"\n            name=\"userName\"\n            placeholder=\"Your user name\"\n            onChange={signInHandler}\n            required\n          />\n        </FormControl>\n        <FormControl isRequired id=\"userName\" py={4}>\n          <FormLabel className=\"form-label\">Password</FormLabel>\n          <InputGroup size=\"md\">\n            <Input\n              color=\"white\"\n              pr=\"4.5rem\"\n              name=\"password\"\n              type={show ? \"text\" : \"password\"}\n              placeholder=\"Enter password\"\n              onChange={signInHandler}\n            />\n            <InputRightElement width=\"4.5rem\">\n              <Button h=\"1.75rem\" size=\"sm\" onClick={handleClick}>\n                {show ? <AiFillEyeInvisible /> : <AiFillEye />}\n              </Button>\n            </InputRightElement>\n          </InputGroup>\n        </FormControl>\n        <Flex borderTop=\"2px\">\n          <Button px=\"6\" mt={4} onClick={submitHandler}>\n            <Link\n              className=\"link\"\n              to={(isUser && `/User/${user[0].userName}`) || \"/\"}\n            >\n              Log In\n            </Link>\n          </Button>\n          <Spacer />\n          <Text\n            mt=\"5\"\n            color=\"teal.400\"\n            fontSize=\"md\"\n            fontWeight=\"bold\"\n            textShadow=\"1px 0 1px #000000\"\n          >\n            Haven't created an account yet?\n          </Text>\n          <Spacer />\n          <Button colorScheme=\"teal\" px=\"6\" mt={4}>\n            <Link className=\"link\" to=\"/signup\">\n              Sign Up\n            </Link>\n          </Button>\n        </Flex>\n      </div>\n    </form>\n  );\n};\n\nexport default LogIn;\n","/home/dci/learningToProgram/Web-Development/DCI/SPA/1st-Project/Food-Rating-App/food-rating-app/src/components/Home.jsx",[],"/home/dci/learningToProgram/Web-Development/DCI/SPA/1st-Project/Food-Rating-App/food-rating-app/src/components/authentication/SignUp.jsx",["95","96"],"import React, { useState } from \"react\";\nimport { Redirect } from \"react-router-dom\";\nimport firebase from \"../../firebase/config\";\n\nimport {\n  Input,\n  Button,\n  FormControl,\n  FormLabel,\n  InputGroup,\n  InputRightElement,\n  Heading,\n  Flex,\n  Spacer,\n  Text,\n  Link,\n} from \"@chakra-ui/react\";\n\nconst SignUp = () => {\n  const [currentUser, setCurrentUser] = useState(null);\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    const { email, password } = e.target.elements;\n    try {\n      firebase\n        .auth()\n        .createUserWithEmailAndPassword(email.value, password.value);\n      setCurrentUser(true);\n    } catch (error) {\n      alert(error);\n    }\n  };\n  if (currentUser) {\n    return <Redirect to=\"/dashboard\" />;\n  }\n  return (\n    <form className=\"logInContainer\">\n      <div className=\"submitContainer\">\n        <Heading\n          as=\"h1\"\n          my=\"5\"\n          color=\"teal.400\"\n          fontSize=\"4xl\"\n          fontWeight=\"extrabold\"\n          textAlign=\"center\"\n        >\n          Create Your Account\n        </Heading>\n        <FormControl onSubmit={handleSubmit}>\n          <FormLabel className=\"form-label\" htmlFor=\"email\">\n            Email\n          </FormLabel>\n          <Input color=\"white\" type=\"email\" name=\"email\" placeholder=\"Email\" />\n        </FormControl>\n        <FormControl py=\"4\">\n          <FormLabel className=\"form-label\" htmlFor=\"password\">\n            Password\n          </FormLabel>\n          <Input\n            color=\"white\"\n            type=\"password\"\n            name=\"password\"\n            placeholder=\"Password\"\n          />\n        </FormControl>\n        <Flex borderTop=\"2px\">\n          <Link className=\"link\" to=\"/\">\n            <Button px=\"6\" mt={4} type=\"submit\">\n              Submit\n            </Button>\n          </Link>\n          <Spacer />\n          <Text\n            mt=\"5\"\n            color=\"teal.400\"\n            fontSize=\"md\"\n            fontWeight=\"bold\"\n            textShadow=\"1px 0 1px #000000\"\n          >\n            Already have an account?\n          </Text>\n          <Spacer />\n          <Link className=\"link\" to=\"/login\">\n            <Button colorScheme=\"teal\" px=\"6\" mt={4}>\n              Log In\n            </Button>\n          </Link>\n        </Flex>\n      </div>\n    </form>\n  );\n};\n\nexport default SignUp;\n","/home/dci/learningToProgram/Web-Development/DCI/SPA/1st-Project/Food-Rating-App/food-rating-app/src/firebase/config.js",[],"/home/dci/learningToProgram/Web-Development/DCI/SPA/1st-Project/Food-Rating-App/food-rating-app/src/components/AddMealForm.jsx",[],"/home/dci/learningToProgram/